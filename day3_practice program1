sal=int(input())
bank=int(input())
hostel=int(input())
food=int(input())
parents=int(input())
spend=bank+hostel+food+parents
if(spend<sal):
    print("He can save the money")
elif(spend==sal):
    print("He can manage the money")
else:
    print("He want to work more")
